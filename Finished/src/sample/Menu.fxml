    <columnConstraints>
        <ColumnConstraints minWidth="-Infinity" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="-Infinity" />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="276.0" prefWidth="452.0">
            <children>
                <Button fx:id="playButton" layoutX="148.0" layoutY="56.0" mnemonicParsing="false" onAction="#handleButtonclick" prefHeight="65.0" prefWidth="149.0" text="PLAY" />
                <Button fx:id="exitButton" layoutX="202.0" layoutY="192.0" mnemonicParsing="false" onAction="#exit" text="Exit" textFill="RED"/>
            </children>
        </AnchorPane>
    </children>
</GridPane>
